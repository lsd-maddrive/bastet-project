{
    "configurations": [
        {
            "name": "windows-gcc-x64",
            "intelliSenseMode": "windows-gcc-x64",
            "compilerPath": "gcc",
            "cStandard": "${default}",
            "cppStandard": "${default}",
            "includePath": [
                "${workspaceFolder}/**"
            ],
            "compilerArgs": [
                "-g",
                "-Os",
